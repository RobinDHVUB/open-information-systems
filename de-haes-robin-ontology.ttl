#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/belonto#Address
:Address rdf:type owl:Class ;
         rdfs:comment """The address of an airport.

This entity could also be used more generally to specify the address of any person or organization."""@en ;
         rdfs:label "Address"@en .


###  http://www.semanticweb.org/belonto#Airplane
:Airplane rdf:type owl:Class ;
          rdfs:comment "An airplane is a flying vehicle that can be used to transport people."@en ;
          rdfs:label "Airplane"@en .


###  http://www.semanticweb.org/belonto#Airport
:Airport rdf:type owl:Class ;
         rdfs:comment "A complex of runways and buildings for the take-off, landing, and maintenance of airplanes, with facilities for passengers."@en ;
         rdfs:label "Airport"@en .


###  http://www.semanticweb.org/belonto#AwardTicket
:AwardTicket rdf:type owl:Class ;
             rdfs:comment "A ticket giving you access to an award, e.g., a free upgrade to business class, that you can obtain in exchange for a specified amount of frequent flyer points that you have previously earned as part of a loyalty program."@en ;
             rdfs:label "Award Ticket"@en ;
             rdfs:seeAlso :LoyaltyProgram .


###  http://www.semanticweb.org/belonto#AwardedPoints
:AwardedPoints rdf:type owl:Class ;
               rdfs:comment "The points that have been awarded to a frequent flyer card by a booking that was made."@en ;
               rdfs:label "Awarded Points"@en ;
               rdfs:seeAlso :FlightBooking ,
                            :FrequentFlyerCard .


###  http://www.semanticweb.org/belonto#Flight
:Flight rdf:type owl:Class ;
        rdfs:comment """A journey via airplane, organized by an airline, to fly passengers from one airport to another.

This type of flight starts as soon as the plane leaves its origin airport and ends once it arrives at another airport. A \"flight\" with layovers is therefore seen as a sequence of connected flights and not as a single flight. Such a sequence of flights is connected via a booking."""@en ;
        rdfs:label "Flight"@en ;
        rdfs:seeAlso :FlightBooking .


###  http://www.semanticweb.org/belonto#FlightBooking
:FlightBooking rdf:type owl:Class ;
               rdfs:subClassOf :Reservation ;
               rdfs:comment "A reservation for a round-trip or direct flight (with potential layovers) for a passenger or group of passengers."@en ;
               rdfs:label "Flight Booking"@en .


###  http://www.semanticweb.org/belonto#FrequentFlyerCard
:FrequentFlyerCard rdf:type owl:Class ;
                   rdfs:comment "A card provided to an airline passenger to provide membership in a loyalty program or frequent-flyer program."@en ;
                   rdfs:label "Frequent Flyer Card"@en ;
                   rdfs:seeAlso :LoyaltyProgram .


###  http://www.semanticweb.org/belonto#InsuranceProduct
:InsuranceProduct rdf:type owl:Class ;
                  rdfs:comment """A product provided by an insurance company to protect against a possible eventuality in return for a payment.

In the context of airline travel, this is mostly used as travel insurance protecting against flight cancellations or curtailments."""@en ;
                  rdfs:label "Insurance"@en .


###  http://www.semanticweb.org/belonto#LoyaltyProgram
:LoyaltyProgram rdf:type owl:Class ;
                rdfs:comment """A plan or system under which members can earn rewards for being regular, loyal customers. 

In the context of airline travel, such a loyalty program is generally a frequent-flyer program in which passengers that frequently use that airline are rewarded points."""@en ;
                rdfs:label "Loyalty Program"@en .


###  http://www.semanticweb.org/belonto#LuggageReservation
:LuggageReservation rdf:type owl:Class ;
                    rdfs:subClassOf :Reservation ;
                    rdfs:comment """A reservation of space on an airplane so a passenger is able to transport luggage on a flight with him or her.
This type of reservation is done in the context of a flight booking."""@en ;
                    rdfs:label "Luggage Reservation"@en ;
                    rdfs:seeAlso :FlightBooking ,
                                 :LuggageService .


###  http://www.semanticweb.org/belonto#LuggageService
:LuggageService rdf:type owl:Class ;
                rdfs:comment "A service provided by an airline to register a certain type of luggage to be transported with a passenger to its destination."@en ;
                rdfs:label "Luggage Service"@en .


###  http://www.semanticweb.org/belonto#MealService
:MealService rdf:type owl:Class ;
             rdfs:comment "A meal that is available for purchase or is provided on a flight as a service to the passengers."@en ;
             rdfs:label "Meal Service"@en .


###  http://www.semanticweb.org/belonto#Passenger
:Passenger rdf:type owl:Class ;
           rdfs:comment "A passenger on a flight."@en ;
           rdfs:label "Passenger"@en .


###  http://www.semanticweb.org/belonto#PaymentOption
:PaymentOption rdf:type owl:Class ;
               rdfs:comment "The option of payment chosen by a passenger."@en ;
               rdfs:label "Payment Option"@en .


###  http://www.semanticweb.org/belonto#Reservation
:Reservation rdf:type owl:Class ;
             rdfs:comment """The reservation of a specific service or product.

This is not used to provide information about the specific service or product that is reserved, but rather to provide information about the actual reservation itself."""@en ;
             rdfs:label "Reservation"@en .


###  http://www.semanticweb.org/belonto#Seat
:Seat rdf:type owl:Class ;
      rdfs:comment "A passenger seat in an airplane, which can be reserved for a flight."@en ;
      rdfs:label "Seat"@en .


###  http://www.semanticweb.org/belonto#SeatReservation
:SeatReservation rdf:type owl:Class ;
                 rdfs:subClassOf :Reservation ;
                 rdfs:comment """A reservation of a specific seat on a flight.
This type of reservation is done in the context of a flight booking."""@en ;
                 rdfs:label "Seat Reservation"@en ;
                 rdfs:seeAlso :FlightBooking ,
                              :Seat .


#################################################################
#    General axioms
#################################################################

# It seems impossible for an individual to belong to more than one of the specified classes. 
# However, we could foresee extensions in which a FlightBooking, LuggageReservation and SeatReservation are all summarized in a single individual.
# Therefore, the subclasses of Reservation have not been explicitly stated to be disjoint.

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Address
                :Airplane
                :Airport
                :AwardTicket
                :AwardedPoints
                :Flight
                :FrequentFlyerCard
                :InsuranceProduct
                :LoyaltyProgram
                :LuggageService
                :MealService
                :Passenger
                :PaymentOption
                :Reservation
                :Seat
              )
] .